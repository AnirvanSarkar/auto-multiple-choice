                            <property name="can_focus">True</property>
                            <property name="receives_default">True</property>
                            <property name="has_tooltip">True</property>
                            <property name="tooltip-text" translatable="yes">Remove manual modifications for this page. Automatic data capture for this page, if any, won' t be changed.</property>
